{
  "name": "leap-js",
  "version": "0.0.4",
  "description": "reactive data layer library",
  "scripts": {
    "bundle": "browserify streamAPI.js -t babelify -s Leap | uglifyjs -o dist/bundle.min.js",
    "watch": "watchify streamAPI.js -t babelify -s Leap -vd -o dist/bundle.js",
    "lint": "eslint examples/*.js *.js",
    "run-rest": "json-server --watch examples/db.json",
    "run-ws": "node examples/ws-server.js",
    "run-sse": "node examples/sse-server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nikkatalnikov/leap"
  },
  "author": {
      "name": "Nik Katalnikov",
      "email": "prometheusaudio@gmail.com"
  },
  "contributors": [{
      "name": "Dmitriy Shapovalov",
      "email": "boxaldis@gmail.com"
  }],
  "license": "ISC",
  "dependencies": {
    "axios": "^0.9.1",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react":"^3.16.1",
    "eslint-config-airbnb": "^5.0.0",
    "rx-lite": "^4.0.7",
    "uglify-js": "^2.6.1",
    "watchify": "^3.7.0"
  }
}
