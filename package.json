{
  "name": "leap-js",
  "version": "0.0.16",
  "description": "reactive data layer library",
  "keywords": [
    "ws",
    "sse",
    "http",
    "rx",
    "stream",
    "async",
    "reactive"
  ],
  "scripts": {
    "bundle": "browserify streamAPI.js -t babelify -s Leap | derequire > dist/leap.js",
    "minify": "uglifyjs dist/leap.js > dist/leap.min.js",
    "watch": "watchify streamAPI.js -t babelify -s Leap -vd -o 'derequire > dist/leap.js'",
    "lint": "eslint *.js",
    "test": "echo Coming soon...",
    "postinstall": "npm run bundle && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nikkatalnikov/leap"
  },
  "author": {
    "name": "Nik Katalnikov",
    "email": "prometheusaudio@gmail.com"
  },
  "contributors": [{
    "name": "Dmitriy Shapovalov",
    "email": "boxaldis@gmail.com"
  }],
  "license": "ISC",
  "devDependencies": {
    "eslint": "^2.11.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "axios": "^0.12.0",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "derequire": "^2.0.3",
    "rxjs": "^5.0.0-beta.9",
    "uglify-js": "^2.6.1"
  }
}
